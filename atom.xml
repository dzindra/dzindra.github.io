---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>{{ site.name | strip_html }}</title>
    <link href="{{ site.baseurl | prepend: site.url | append:'/' }}atom.xml" rel="self"/>
    <link href="{{ site.baseurl | prepend: site.url | append:'/' }}"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>{{ site.baseurl | prepend: site.url }}/</id>
    <author>
        <name>{{ site.author | strip_html }}</name>
        {% if site.email %}<email>{{ site.email }}</email>{% endif %}
    </author>

    {% for post in site.posts limit: 20 %}
    <entry>
        <id>{{ site.baseurl | prepend: site.url }}{{ post.id | uri_escape }}</id>
        <title>{{ post.title | strip_html }}</title>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <link rel="alternate" href="{{ site.baseurl | prepend: site.url }}{{ post.url }}"/>
        <summary>{{ post.excerpt | strip_html }}</summary>
    </entry>
    {% endfor %}
</feed>